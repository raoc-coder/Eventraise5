{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "types": ["cypress", "node"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
